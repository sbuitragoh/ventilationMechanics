#:import MeshLinePlot kivy_garden.graph.MeshLinePlot
Logic:
    BoxLayout:
        orientation: "vertical"
        BoxLayout:
            size_hint: [1, .5]
            Graph:
                id: graph
                xlabel: "seg"
                ylabel: "cm H20"
        BoxLayout:
            size_hint: [1, .5]
            Graph:
                id: graph2
                xlabel: "seg"
                ylabel: "L/min"
        BoxLayout:
            size_hint: [1, .5]
            Graph:
                id: graph3
                xlabel: "seg"
                ylabel: "L"
        BoxLayout:
            size_hint: [1, .1]
            orientation: "horizontal"
            Button:
                text: "START"
                bold: True
                on_press: root.start()
            Button:
                text: "STOP"
                bold: True
                on_press: root.stop()